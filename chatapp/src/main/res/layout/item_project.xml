<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.zhy.autolayout.AutoRelativeLayout
        android:id="@+id/rl_back"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/pic_xmbg"
        android:layout_marginBottom="-10dp">

        <TextView
            android:id="@+id/tv_project_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="72px"
            android:layout_marginTop="65px"
            android:textStyle="bold"
            android:textSize="16sp"
            android:text="1111111111111"
            android:textColor="@color/black" />


        <ImageView
            android:id="@+id/im_icons1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/tv_project_name"
            android:layout_below="@+id/tv_project_name"
            android:layout_marginStart="0dp"
            android:layout_marginTop="20px"
            android:src="@drawable/icon_gsname" />

        <TextView
            android:id="@+id/tv_gsname"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_project_name"
            android:layout_marginLeft="20px"
            android:layout_marginTop="20px"
            android:layout_toEndOf="@+id/im_icons1"
            android:textColor="@color/black1"
            android:ellipsize="end"
            android:singleLine="true"
            android:textSize="12sp" />


        <ImageView
            android:id="@+id/im_icons2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="20px"
            android:layout_below="@+id/tv_project_name"
            android:src="@drawable/icon_xmfzr" />


        <TextView
            android:id="@+id/tv_fuzeren"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_project_name"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="20px"
            android:layout_marginTop="20px"
            android:layout_toEndOf="@+id/im_icons2"
            android:textColor="@color/black1"
            android:textSize="12sp" />


        <ImageView
            android:id="@+id/im_icons3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_project_name"
            android:layout_marginTop="20px"
            android:layout_toStartOf="@+id/tv_date"
            android:src="@drawable/icon_qdsj" />


        <TextView
            android:id="@+id/tv_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_project_name"
            android:layout_marginLeft="20px"
            android:layout_marginTop="20px"
            android:layout_alignParentRight="true"
            android:layout_marginRight="72px"
            android:textColor="@color/black1"
            android:textSize="12sp" />


        <ImageView
            android:id="@+id/im_icons4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/tv_project_name"
            android:layout_below="@+id/im_icons1"
            android:layout_marginTop="30px"
            android:src="@drawable/icon_gsname" />

        <TextView
            android:id="@+id/tv_content"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/im_icons1"
            android:layout_marginLeft="20px"
            android:layout_marginTop="30px"
            android:layout_toEndOf="@+id/im_icons4"
            android:textColor="@color/black1"
            android:ellipsize="end"
            android:singleLine="true"
            android:textSize="12sp" />


        <ImageView
            android:id="@+id/im_icons5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/im_icons4"
            android:layout_centerHorizontal="true"
            android:src="@drawable/icon_jdwt" />

        <TextView
            android:id="@+id/tv_jieguo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/im_icons1"
            android:layout_marginLeft="20px"
            android:layout_marginTop="30px"
            android:layout_toEndOf="@+id/im_icons5"
            android:maxLength="14"
            android:textColor="@color/black1"
            android:textSize="12sp" />


    </com.zhy.autolayout.AutoRelativeLayout>
</com.zhy.autolayout.AutoLinearLayout>